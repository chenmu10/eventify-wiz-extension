{
  "manifest_version": 3,
  "name": "EventifyWiz - Text to Event",
  "description": "A chrome extension to create new google calendar events from any text",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "calendar-icon-34.png"
  },
  "icons": {
    "34": "calendar-icon-34.png",
    "128": "calendar-icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "js": ["contentScript.bundle.js"],
      "css": ["content.styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["content.styles.css", "calendar-icon-128.png", "calendar-icon-34.png"],
      "matches": []
    }
  ]
}
